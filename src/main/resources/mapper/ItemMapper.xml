<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lcomputerstudy.example.mapper.ItemMapper">
	<select id="getItemList" resultType ="Item">
	SELECT 		*
	FROM 		Item
	
	</select>
	
	<select id="detailTopList" resultType="Top">
		SELECT i_idx,top_idx,name
		FROM top

		
	</select>
	<insert id="itemInsert" parameterType="Item">
		INSERT INTO item(
			i_idx,
			i_name
		)VALUES(
			#{i_idx},
			#{i_name}
		)
		
	</insert>
	<insert id="topInsert" parameterType="Top">
		INSERT INTO top(
			i_idx,
			top_idx,
			name
			
		)VALUES(
		    #{i_idx},
		    #{top_idx},
		    #{name}
		
		)
	</insert>






</mapper>